<%= form_with(model: siaxle) do |f| %>

    <%= render "shared/errors", object: siaxle %>
  
    <%= f.label :name %>
    <%= f.text_area :name, rows: 4, autofocus: true %>
  
    <%= f.label :description %>
    <%= f.text_area :description, rows: 12 %>
  
    <%= f.label :time %>
    <%= f.datetime_select :starts_at, {ampm: true}, {class: "datetime"} %>
  
    <%= f.label :sia_image %>
    <%= f.file_field :sia_image %>
  
    <%= f.submit %>
  <% end %>
  